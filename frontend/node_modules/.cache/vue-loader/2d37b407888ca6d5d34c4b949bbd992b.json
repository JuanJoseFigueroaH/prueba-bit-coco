            axios.post('producto/store',\r\n                {\r\n                    'codigo': this.codigo,\r\n                    'nombre': this.nombre,\r\n                })\r\n                .then(function(response){\r\n                    me.limpiar();\r\n                    me.close();\r\n                    me.listar();\r\n                })\r\n                .catch(function(error){\r\n                    console.log(error);\r\n                });\r\n            },\r\n            close () {\r\n                this.dialog = false;\r\n            }\r\n        }\r\n    }\r\n</script>\r\n"]}]}